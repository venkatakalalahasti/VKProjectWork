<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/pe/SignalInActivitySchema">
    <pd:name>Test a file transfer scenario/Wait for File Change/Wait for File Change in A.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startX>150</pd:startX>
    <pd:startY>64</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>565</pd:endX>
    <pd:endY>68</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1566294579281</pd:targetNamespace>
    <pd:activity name="Wait for File Change">
        <pd:type>com.tibco.plugin.file.FileSignalIn</pd:type>
        <pd:resourceType>ae.activities.FileSignalInUI</pd:resourceType>
        <pd:x>310</pd:x>
        <pd:y>73</pd:y>
        <config>
            <_eventTimeout>60000</_eventTimeout>
            <pollInterval>5</pollInterval>
            <createEvent>true</createEvent>
            <modifyEvent>true</modifyEvent>
            <deleteEvent>true</deleteEvent>
            <mode>files-and-directories</mode>
            <encoding>text</encoding>
            <sortby>sortbyName</sortby>
            <sortorder>sortorderDescending</sortorder>
            <fileName>D:\Sample Files\A\Chaithu.txt</fileName>
        </config>
        <pd:inputBindings>
            <ns:input/>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Wait for File Change</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Wait for File Change</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>