<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:ns2="www.tibco.com/plugin/Timer" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns1="http://www.tibco.com/pe/EngineTypes" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:ns3="www.tibco.com/plugin/Sleep" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx="http://www.tibco.com/schemas/Sample Project 19th August 2019/Parsers/Schema.xsd">
    <xsd:import namespace="http://www.tibco.com/schemas/Sample Project 19th August 2019/Parsers/Schema.xsd" schemaLocation="/Parsers/Employee Data Schema.xsd"/>
    <pd:name>Functions and General Acivities/Flat to XML - Multiple Functions.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startX>87</pd:startX>
    <pd:startY>79</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>740</pd:endX>
    <pd:endY>83</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1566304030120</pd:targetNamespace>
    <pd:activity name="Parse E File">
        <pd:type>com.tibco.plugin.parse.ParseActivity</pd:type>
        <pd:resourceType>ae.activities.ParseActivity</pd:resourceType>
        <pd:x>210</pd:x>
        <pd:y>81</pd:y>
        <config>
            <InputType>File</InputType>
            <Encoding>ASCII</Encoding>
            <ParseSharedConfig>/Parsers/Employee Format.sharedparse</ParseSharedConfig>
        </config>
        <pd:inputBindings>
            <Input>
                <fileName>
                    <xsl:value-of select="&quot;D:\Sample Files\Parser Files\Employee 1.txt&quot;"/>
                </fileName>
                <noOfRecords>
                    <xsl:value-of select="-1"/>
                </noOfRecords>
            </Input>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Render E File">
        <pd:type>com.tibco.plugin.xml.XMLRendererActivity</pd:type>
        <pd:resourceType>ae.activities.XMLRendererActivity</pd:resourceType>
        <pd:x>393</pd:x>
        <pd:y>82</pd:y>
        <config>
            <renderAsText>text</renderAsText>
            <validateInput>true</validateInput>
            <term ref="pfx:CompanySch"/>
        </config>
        <pd:inputBindings>
            <pfx:CompanySch>
                <xsl:for-each select="$Parse-E-File/Output/Rows/Employee-Data">
                    <pfx:EmployeeDataSch>
                        <pfx:ENameSch>
                            <xsl:value-of select="upper-case(E-Name)"/>
                        </pfx:ENameSch>
                        <pfx:EIDSch>
                            <xsl:value-of select="concat(substring(E-ID, 2,1),substring(E-ID, 6,1))"/>
                        </pfx:EIDSch>
                        <pfx:EMobSch>
                            <xsl:value-of select="E-Mob"/>
                        </pfx:EMobSch>
                        <pfx:ELocSch>
                            <xsl:value-of select="E-Location"/>
                        </pfx:ELocSch>
                        <pfx:EBGroupSch>
                            <xsl:value-of select="E-BGroup"/>
                        </pfx:EBGroupSch>
                    </pfx:EmployeeDataSch>
                </xsl:for-each>
            </pfx:CompanySch>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Write XML">
        <pd:type>com.tibco.plugin.file.FileWriteActivity</pd:type>
        <pd:resourceType>ae.activities.FileWriteActivity</pd:resourceType>
        <pd:x>585</pd:x>
        <pd:y>83</pd:y>
        <config>
            <encoding>text</encoding>
            <compressFile>None</compressFile>
            <createMissingDirectories>true</createMissingDirectories>
        </config>
        <pd:inputBindings>
            <ns:WriteActivityInputTextClass>
                <fileName>
                    <xsl:value-of select="&quot;D:\Sample Files\Parser Files11221\ABC.xml&quot;"/>
                </fileName>
                <textContent>
                    <xsl:value-of select="$Render-E-File/xmlString"/>
                </textContent>
            </ns:WriteActivityInputTextClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Catch Any Error">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>345</pd:x>
        <pd:y>242</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>false</catchAll>
            <fault>localname=BadDataFormatException</fault>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Write File">
        <pd:type>com.tibco.plugin.file.FileWriteActivity</pd:type>
        <pd:resourceType>ae.activities.FileWriteActivity</pd:resourceType>
        <pd:x>558</pd:x>
        <pd:y>240</pd:y>
        <config>
            <encoding>text</encoding>
            <compressFile>None</compressFile>
            <append>true</append>
        </config>
        <pd:inputBindings>
            <ns:WriteActivityInputTextClass>
                <fileName>
                    <xsl:value-of select="&quot;D:\Sample Files\Error Files\Error in Flat to XML.txt&quot;"/>
                </fileName>
                <textContent>
                    <xsl:value-of select="concat(current-dateTime(), $_error/ns1:ErrorReport/Msg)"/>
                </textContent>
            </ns:WriteActivityInputTextClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Render E File</pd:from>
        <pd:to>Write XML</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch Any Error</pd:from>
        <pd:to>Write File</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Write File</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>One Bend</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Write XML</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Parse E File</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Parse E File</pd:from>
        <pd:to>Render E File</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>